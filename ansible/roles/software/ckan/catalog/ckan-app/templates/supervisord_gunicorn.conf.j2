[program:catalog-web]
command={{ project_source_new_code_path }}/bin/gunicorn --paste /etc/ckan/production.ini -b localhost:5000
directory={{ project_source_new_code_path }}
user=www-data
group=www-data
stdout_logfile={{ catalog_gunicorn_error_log }}
autostart=true
autorestart=true
redirect_stderr=true